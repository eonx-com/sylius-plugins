{
    "name": "eonx-com/payments-plugin",
    "type": "sylius-plugin",
    "description": "EonX Payments plugin for Sylius.",
    "keywords": ["sylius", "sylius-plugin"],
    "license": "MIT",
    "require": {
        "php": "^7.4 || ^8.0",
        "sylius/sylius": "~1.8.0 || ~1.9.0 || ~1.10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "EonX\\PaymentsPlugin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\PaymentsPlugin\\Tests\\": "tests/"
        }
    }
}
